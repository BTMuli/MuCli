{
	"name": "@btmuli/mucli",
	"version": "0.8.0",
	"subversion": {
		"dev": "0.2.2",
		"mmd": "0.7.7",
		"pip": "0.4.3",
		"typora": "0.7.3"
	},
	"description": "个人用的 Node Cli",
	"packageManager": "yarn@3.6.1",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"build": "rollup -c",
		"lint-all": "eslint --ext .ts,.json ./",
		"lint-fix": "eslint --ext .ts,.json ./ --fix",
		"postinstall": "husky install",
		"prepublishOnly": "pinst --disable",
		"postpublish": "pinst --enable"
	},
	"lint-staged": {
		"*.ts": "eslint --fix",
		"*.{ts,md,json,yml}": "prettier --write"
	},
	"bin": {
		"muc": "./dist/index.js"
	},
	"files": [
		"config_default",
		"dist"
	],
	"keywords": [
		"Node",
		"Cli",
		"BTMuli"
	],
	"author": "目棃",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/BTMuli/MuCli.git"
	},
	"bugs": {
		"url": "https://github.com/BTMuli/MuCli/issues"
	},
	"homepage": "https://github.com/BTMuli/MuCli#readme",
	"dependencies": {
		"axios": "^1.4.0",
		"commander": "^11.0.0",
		"inquirer": "^9.2.7",
		"silly-datetime": "^0.1.2",
		"typescript": "^5.1.3",
		"yamljs": "^0.3.0"
	},
	"devDependencies": {
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.1.0",
		"@rollup/plugin-terser": "^0.4.3",
		"@types/inquirer": "^9.0.3",
		"@types/node": "^20.3.1",
		"@types/yamljs": "^0.2.31",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.8",
		"rollup": "^3.25.1",
		"rollup-plugin-typescript2": "^0.34.1"
	}
}
